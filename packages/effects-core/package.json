{
  "name": "@galacean/effects-core",
  "version": "2.0.5",
  "description": "Galacean Effects runtime core for the web",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "pnpm build:fbs && pnpm build:declaration && pnpm build:module",
    "build:fbs": "node ../../scripts/flatc ./src/__definations__/index.fbs",
    "build:module": "rollup -c",
    "build:declaration": "tsc -d --emitDeclarationOnly",
    "clean": "rimraf dist && rimraf \"*+(.tsbuildinfo)\"",
    "prepublishOnly": "pnpm build"
  },
  "contributors": [
    {
      "name": "燃然"
    },
    {
      "name": "飂兮"
    },
    {
      "name": "十弦"
    },
    {
      "name": "云垣"
    },
    {
      "name": "茂安"
    },
    {
      "name": "意绮"
    }
  ],
  "author": "Ant Group CO., Ltd.",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@galacean/effects-specification": "2.0.1",
    "@galacean/effects-math": "1.1.0",
    "flatbuffers": "24.3.25",
    "uuid": "9.0.1",
    "libtess": "1.2.2"
  }
}
